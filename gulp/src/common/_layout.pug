include _mixin
append item
block variables
doctype html
html(lang=pageLang)
  head(prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# " + pageOgpType + ": http://ogp.me/ns/" + pageOgpType + "#")
    include _meta
  body(class=pageClass)
    .wrapper
      block header
        include _header
      .wrapper__contents
        block contents
      block footer
        include _footer
    block js
      script(type="text/javascript" src="https://code.jquery.com/jquery-3.0.0.min.js")
      script(type="text/javascript" src=pageUrl+"js/functions.js")
