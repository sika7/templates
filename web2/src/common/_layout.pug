doctype html
append item
block variables
include _site.pug
include _mixin.pug
html(lang=pageLang)
  head(prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# " + pageOgpType + ": http://ogp.me/ns/" + pageOgpType + "#")
    include _meta.pug
  body(class=pageClass)
    .wrapper
      block header
        include _header.pug
      .wrapper__contents
        block contents
      block footer
        include _footer.pug
    block foot
      script(type="text/javascript" src=relativePath+"js/common.jsx")